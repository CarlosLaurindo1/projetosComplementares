package modelo;

import visualizacao.*;

public class Movimentacao {

	private int transacao;
	private static String titular;
	private static String dataNascimento, dataMov;
	private static long cpf;
	private double valor;
	private double saldo;
	private String data;
	private int totalTransacao;
	private static String tipoConta;

	public int getTransacao() {
		return transacao;
	}

	public void setTransacao(int tipo) {
		this.transacao = transacao;
	}

	public double getValorDep() {
		return valor;
	}

	public void setValorDep(double valor) {
		this.valor = valor;
	}

	public double getValorSaq() {
		return valor;
	}

	public void setValorSaq(double valor) {
		this.valor = valor;
	}

	public double getSaldo() {
		return saldo;
	}

	public void setSaldo(double saldo) {
		this.saldo = saldo;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public static String getTitular() {
		return titular;
	}

	public void setTitular(String titular) {
		this.titular = titular;
	}

	public static String getTipoConta() {
		return tipoConta;
	}

	public void setTipoConta(String tipoConta) {
		this.tipoConta = tipoConta;
	}

	public static String getDataNascimento() {
		return dataNascimento;
	}

	public void setDataNascimento(String dataNascimento) {
		this.dataNascimento = dataNascimento;
	}

	public static long getCpf() {
		return cpf;
	}

	public void setCpf(long cpf) {
		this.cpf = cpf;
	}

	public static String getDataMov() {
		return dataMov;
	}

	public void setDataMov() {
		this.dataMov = dataMov;
	}

	public void constroiExtratoCompleto(int transacao, double valor, String data) {
		setTransacao(transacao);
		setValorDep(valor);
		setData(data);
	}

}
