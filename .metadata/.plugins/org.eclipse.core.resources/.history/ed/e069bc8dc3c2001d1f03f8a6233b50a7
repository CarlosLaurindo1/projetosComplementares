package modelo;

import visualizacao.*;

public class Movimentacao {

	private int transacao;
	private static String titular;
	private static String dataNascimento, dataMov;
	private static long cpf;
	private String data;
	private static String tipoConta;
	private double valorDep;
	private double valorSaq;

	public int getTransacao() {
		return transacao;
	}

	public void setTransacao(int transacao) {
		this.transacao = transacao;
	}

	public double getValorDep() {
		return valorDep;
	}

	public void setValorDep(double valorDep) {
		this.valorDep = valorDep;
	}

	public double getValorSaq() {
		return valorSaq;
	}

	public void setValorSaq(double valorSaq) {
		this.valorSaq = valorSaq;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public String getTitular() {
		return titular;
	}

	public void setTitular(String titular) {
		this.titular = titular;
	}

	public String getTipoConta() {
		return tipoConta;
	}

	public void setTipoConta(String tipoConta) {
		this.tipoConta = tipoConta;
	}

	public String getDataNascimento() {
		return dataNascimento;
	}

	public void setDataNascimento(String dataNascimento) {
		this.dataNascimento = dataNascimento;
	}

	public long getCpf() {
		return cpf;
	}

	public void setCpf(long cpf) {
		this.cpf = cpf;
	}

	public String getDataMov() {
		return dataMov;
	}

	public void setDataMov() {
		this.dataMov = dataMov;
	}


}
